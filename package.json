{
  "name": "sureify-recruitment-task",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.3.1",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.3",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "compile:sass": "node-sass src/styles/main.scss src/styles/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/styles/style.comp.css -o src/styles/style.prefix.css",
    "compress:css": "node-sass src/styles/style.prefix.css src/index.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "watch-css": "node-sass src/styles/main.scss src/index.css -w",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.11.0",
    "redux-mock-store": "^1.5.3"
  }
}
